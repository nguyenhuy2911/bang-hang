@model StockManager.Web.Models.IMPORT_FormList_Model

@{
    ViewBag.Title = "Import_List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="col-md-12">

    <button id="@(Model.FunctionName)_btn_New" type="button" class="btn btn-primary pull-right" onclick="newImportForm()">
        <i class="fa fa-plus-circle" aria-hidden="true"></i>
        @Common.Utilies.getResourceString("AddNew")
    </button>
</div>
<div class="custometab clear">
    <div class="tab" role="tabpanel">
        <!-- Nav tabs -->
        <ul class="nav nav-tabs" role="tablist">
            <li role="presentation" class="active">
                <a href="#@(Model.FunctionName)_div_Indate" aria-controls="home" role="tab" data-toggle="tab">
                    <i class="fa fa-file-text"></i>@Common.Utilies.getResourceString("Inday")
                </a>
            </li>
            <li role="presentation">
                <a href="#@(Model.FunctionName)_div_History" aria-controls="profile" role="tab" data-toggle="tab">
                    <i class="fa fa-book"></i>@Common.Utilies.getResourceString("History")
                </a>
            </li>
        </ul>
        <!-- Tab panes -->
        <div class="tab-content">
            <div role="tabpanel" class="tab-pane fade in active" id="@(Model.FunctionName)_div_Indate">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel col-md-12 col-sm-12 col-xs-12">
                        <div class="x_title">
                            <h2>Danh sách<small></small></h2>
                            <ul class="nav navbar-right panel_toolbox">
                                <li>
                                    <a class="btn btn-link filter-bar" data-toggle="collapse" data-target="#searh-bar-div-dataList"><i class="fa fa-3x fa-filter"></i> Tìm kiếm</a>
                                </li>
                                <li>
                                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                                </li>
                            </ul>
                            <div class="clearfix"></div>
                            <div id="searh-bar-div-dataList" class="collapse col-md-12 col-sm-12 col-xs-12">
                                <fieldset class="fieldset">
                                    <legend>Thông tin tìm kiếm</legend>
                                    <div class="row-fluid">
                                        <form class="form-horizontal form-label-left">
                                            <div class="form-group">
                                                <div class="col-md-2 col-sm-2 col-xs-12">
                                                    <input type="text" placeholder="" class="form-control col-md-12 col-sm-12 col-xs-12">
                                                </div>
                                                <div class="col-md-2 col-sm-2 col-xs-12">
                                                    <input type="text" placeholder="" class="form-control col-md-12 col-sm-12 col-xs-12">
                                                </div>
                                                <div class="col-md-2 col-sm-2 col-xs-12">
                                                    <button id="@(Model.FunctionName)_btn_Search" type="button" class="btn btn-dark col-md-12 col-sm-12 col-xs-12">
                                                        <i class="fa fa-search" aria-hidden="true"></i>@Common.Utilies.getResourceString("Search")
                                                    </button>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </fieldset>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content ">
                            <div class="col-md-12 col-sm-12 col-xs-12" id="@(Model.FunctionName)_div_dataList">
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <div role="tabpanel" class="tab-pane fade" id="@(Model.FunctionName)_div_History">
                <div class="col-md-12 col-sm-12 col-xs-12">
                    <div class="x_panel col-md-12 col-sm-12 col-xs-12">
                        <div class="x_title">
                            <h2>Danh sách<small></small></h2>
                            <ul class="nav navbar-right panel_toolbox">
                                <li>
                                    <a class="btn btn-link filter-bar" data-toggle="collapse" data-target="#searh-bar-div-dataListHistory"><i class="fa fa-3x fa-filter"></i> Tìm kiếm</a>
                                </li>
                                <li>
                                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                                </li>
                            </ul>
                            <div class="clearfix"></div>
                            <div id="searh-bar-div-dataListHistory" class="collapse col-md-12 col-sm-12 col-xs-12">
                                <fieldset class="fieldset">
                                    <legend>Thông tin tìm kiếm</legend>
                                    <div class="row-fluid">
                                        <form class="form-horizontal form-label-left">
                                            <div class="form-group">
                                                <div class="col-md-2 col-sm-2 col-xs-12">
                                                    <input type="text" placeholder="" class="form-control col-md-12 col-sm-12 col-xs-12">
                                                </div>
                                                <div class="col-md-2 col-sm-2 col-xs-12">
                                                    <input type="text" placeholder="" class="form-control col-md-12 col-sm-12 col-xs-12">
                                                </div>
                                                <div class="col-md-2 col-sm-2 col-xs-12">
                                                    <button id="@(Model.FunctionName)_btn_Search" type="button" class="btn btn-dark col-md-12 col-sm-12 col-xs-12">
                                                        <i class="fa fa-search" aria-hidden="true"></i>@Common.Utilies.getResourceString("Search")
                                                    </button>
                                                </div>
                                            </div>
                                        </form>
                                    </div>
                                </fieldset>
                            </div>
                            <div class="clearfix"></div>
                        </div>
                        <div class="x_content ">
                            <div class="col-md-12 col-sm-12 col-xs-12" id="@(Model.FunctionName)_div_dataListHistory">
                            </div>
                        </div>
                    </div>
                </div>


            </div>
        </div>
    </div>
</div>



<script type="text/javascript">
    var object_IndayForm_GridHeader = html2text('@Model.IndayForm_GridHeader');
    var object_HistoryForm_GridHeader = html2text('@Model.HistoryForm_GridHeader');
</script>
<script src="~/Scripts/Function/IMPORT/IMPORT.js"></script>
<script type="text/javascript">
    $(function () {
        var _module = new IMPORT();
        _module.bindData_Inday(null);
        _module.bindData_History();
    });
</script>
