        <div class="x_title">
            <h2>Thông tin phiếu nhập<small></small></h2>
            <ul class="nav navbar-right panel_toolbox">
                <li>
                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                </li>
            </ul>
            <div class="clearfix"></div>
        </div>
        <div class="x_content ">
            <form class="form-horizontal form-label-left">
                <div class="form-group">
                    <label class="control-label font-normal col-md-2 col-sm-2 col-xs-12">Nhân viên: </label>
                    <label class="control-label col-md-2 col-sm-2 col-xs-12">
                        xxx
                    </label>
                    <label class="control-label font-normal col-md-1 col-sm-1 col-xs-12">Ngày:  </label>
                    <label class="control-label col-md-2 col-sm-2 col-xs-12">
                        xxx
                    </label>
                </div>
                <div class="form-group">
                    <label class="control-label font-normal col-md-2 col-sm-2 col-xs-12">
                        Tên Kho: @*<span class="required">*</span>*@
                    </label>
                    <div class="col-md-3 col-sm-3 col-xs-12">
                        <select class="form-control"></select>
                    </div>

                </div>
                <div class="form-group">
                    <label class="control-label font-normal col-md-2 col-sm-2 col-xs-12">
                        Mô tả: @*<span class="required">*</span>*@
                    </label>
                    <div class="col-md-6 col-sm-6 col-xs-12">
                        <textarea class="form-control" rows="3" placeholder=''></textarea>
                    </div>
                </div>

            </form>
        </div>
    </div>
</div>
