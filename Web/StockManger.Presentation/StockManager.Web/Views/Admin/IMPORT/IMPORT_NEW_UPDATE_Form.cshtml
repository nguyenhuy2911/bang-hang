                    <a class="btn btn-link filter-bar" data-toggle="collapse" data-target="#searh-bar"><i class="fa fa-2x fa-filter"></i> Tìm kiếm</a>
                </li>
                <li>
                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                </li>
            </ul>
            <div class="clearfix"></div>
            <div id="searh-bar" class="collapse col-md-12 col-sm-12 col-xs-12">
                <fieldset class="fieldset">
                    <legend>Thông tin tìm kiếm</legend>
                    <div class="row-fluid">
                        <form class="form-horizontal form-label-left">
                            <div class="form-group">
                                <div class="col-md-4 col-sm-4 col-xs-12">
                                    <input type="text" placeholder="" class="form-control col-md-12 col-sm-12 col-xs-12">
                                </div>
                                <div class="col-md-4 col-sm-4 col-xs-12">
                                    <input type="text" placeholder="" class="form-control col-md-12 col-sm-12 col-xs-12">
                                </div>
                                <div class="col-md-4 col-sm-2 col-xs-12">
                                    <button id="btn_Search" type="button" class="btn btn-dark col-md-12 col-sm-12 col-xs-12">
                                        <i class="fa fa-search" aria-hidden="true"></i>@Common.Utilies.getResourceString("Search")
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                </fieldset>
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="x_content ">
            <form class="form-horizontal form-label-left">
                <div class="form-group">
                    <label class="control-label font-normal col-md2 col-sm-2 col-xs-12">Item code:  </label>
                    <label class="col-md-4 col-sm-4 col-xs-12">
                        <input type="text" class="form-control " />
                    </label>
                    <label class="col-md-4 col-sm-4 col-xs-12">
                        <input type="button" class="btn btn-primary" value="Thêm">
                    </label>
                </div>
            </form>
            <div class="col-md-12 col-sm-12 col-xs-12 form-group">
                <div class="table-responsive">
                    <table class="table table-striped jambo_table bulk_action">
                        <thead>
                            <tr class="headings">
                                <th class="column-title">Tên sản phẩm </th>
                                <th class="column-title">Item Code </th>
                                <th class="column-title">Tồn kho </th>
                                <th class="column-title">Action </th>
                            </tr>
                        </thead>
                        <tbody>
