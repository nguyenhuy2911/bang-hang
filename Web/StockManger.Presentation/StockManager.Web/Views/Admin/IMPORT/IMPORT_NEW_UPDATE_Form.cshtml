@model StockManager.Web.Models.IMPORT_New_UpDate_Form_Model

@{
    ViewBag.Title = "Import_New";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="col-md-12 col-sm-12 col-xs-12">
