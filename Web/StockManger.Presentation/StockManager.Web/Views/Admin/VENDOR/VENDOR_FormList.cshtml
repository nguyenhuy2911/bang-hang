@model StockManager.Web.Models.VENDOR_FormList_Model

@{
    ViewBag.Title = "VENDOR_List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="col-md-12">
   
    <button id="@(Model.FunctionName)_btn_New" type="button" class="btn btn-primary pull-right" onclick="newVendor()">
        <i class="fa fa-plus-circle" aria-hidden="true"></i>@Common.Utilies.getResourceString("AddNew")
    </button>
</div>
<div class="col-md-12 col-sm-12 col-xs-12">
    <div class="x_panel col-md-12 col-sm-12 col-xs-12">
        <div class="x_title">
            <h2>Danh sách<small></small></h2>
            <ul class="nav navbar-right panel_toolbox">
                <li>
                    <a class="btn btn-link filter-bar" data-toggle="collapse" data-target="#searh-bar" href="#"><i class="fa fa-3x fa-filter"></i> Tìm kiếm</a>
                </li>
                 <li>
                    <a class="collapse-link"><i class="fa fa-chevron-up"></i></a>
                </li>
            </ul>
            <div class="clearfix"></div>
            <div id="searh-bar" class="collapse col-md-12 col-sm-12 col-xs-12">
                <fieldset class="fieldset">
                    <legend>Thông tin tìm kiếm</legend>
                    <div class="row-fluid">
                        <form class="form-horizontal form-label-left">
                            <div class="form-group">
                                <div class="col-md-2 col-sm-2 col-xs-12">
                                    <input type="text" placeholder="" class="form-control col-md-12 col-sm-12 col-xs-12">
                                </div>
                                <div class="col-md-2 col-sm-2 col-xs-12">
                                    <input type="text" placeholder="" class="form-control col-md-12 col-sm-12 col-xs-12">
                                </div>
                                <div class="col-md-2 col-sm-2 col-xs-12">
                                    <button id="@(Model.FunctionName)_btn_Search" type="button" class="btn btn-dark col-md-12 col-sm-12 col-xs-12">
                                        <i class="fa fa-search" aria-hidden="true"></i>@Common.Utilies.getResourceString("Search")
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                </fieldset>
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="x_content ">
            <div class="col-md-12 col-sm-12 col-xs-12" id="@(Model.FunctionName)_div_dataList">

            </div>
        </div>
    </div>
</div>
<div class="col-md-12 col-sm-12 col-xs-12" id="@(Model.FunctionName)_div_dataList">

</div>
<script type="text/javascript">
    var objectHeader = html2text('@Model.GridHeader');
</script>
<script src="~/Scripts/Function/VENDOR/VENDOR.js"></script>
<script type="text/javascript">
    $(function () {
        var _module = new VENDOR();
        _module.bindData(null);
    });
</script>
